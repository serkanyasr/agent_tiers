{
  "mcpServers": {
    "memory": {
      "protocol": "http-stream",
      "url": "http://mcp_memory:8050/mcp",
      "headers": {
        "Accept": "text/event-stream"
      }
    },
    "rag": {
      "protocol": "http-stream",
      "url": "http://mcp_rag:8055/mcp",
      "headers": {
        "Accept": "text/event-stream"
      }
    },
    "firecrawl-mcp": {
      "protocol": "stdio",
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": {
        "FIRECRAWL_API_KEY": ""
      }
    }
  }
}
